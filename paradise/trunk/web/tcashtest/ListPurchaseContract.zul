                </columns>
                <rows>
                    <row>
                        <label   value="Supplier" width="50px" />
                        <!--<hbox>-->
                        <!--<textbox id="txtid" visible="false" width="40px" />-->
                        
                        <textbox id="txtsuppliercode" placeholder="Supplier" width="200px" style="background-color:#FFFACD"/>
