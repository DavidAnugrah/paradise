                        <!--<button id="btnitemcode" src="/img/search.png"/>-->
                        <!--</hbox>-->
                    </row>
                    <row>
                       <label   value="Contract" width="50px" />
                       <textbox id="txtContract" placeholder="Contract" width="200px" style="background-color:#FFFACD"/>
                         <!--<hbox>-->
                        <!--<textbox id="txtsupid" visible="false" width="40px" />-->
                        <!--<textbox id="txtdesc" placeholder="Supplier Description" width="140px" style="background-color:#FFFACD"/>-->
                        <!--<button id="btnbucode" src="/img/search.png"/>-->
                        <!--</hbox>-->
                    </row>
                </rows>
               </grid>
              
            <grid oddRowSclass="non-odd" width="420px">
                 <columns>
                    <column label="" width="210px" align="center" />
                    <column label="" width="210px" align="center" />
                </columns>
                <rows>
                 <row>
                     <button id="goFind"  tabindex="12" label="Find"  image="/img/search.png" />
                     <button id="Close1" tabindex="13" label="Close" image="/img/End16.png" />
                 </row>
                </rows>
            </grid>	
           
    </window>
    </window>
    
</zk>
